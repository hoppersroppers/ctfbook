{
  "name": "CTF Book",
  "version": "0.0.1",
  "description": "A Test GitBook",
  "title": "title",
  
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-livereload",
    "-sharing",
    "github-buttons",
    "editlink",
    "ga",
    "-highlight",
    "prism",
    "bibtex-indexed-cite",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small",
  ],
  "repository": {
    "url": "git@github.com:gldraphael/gitbook-test.git",
    "type": "git"
  },
  "author": "Dennis Devey",
  "license": "Copyright",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-load-plugins": "^1.3.0"
  },
  "ga": {
      "token": "UA-151638088-2"
   },
      "page-footer-ex": {
      "copyright": "Roppers Academy &copy 2019",
      "markdown": true,
      "update_label": "Date: ",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
      "github-buttons": {
      "buttons": [{
        "user": "riskers",
        "repo": "gitbook-template",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "editlink": {
      "base": "https://github.com/hoppersroppers/ctfbook/blob/master",
      "label": "Edit",
      "multilingual": false
    },
 
}

